<%- include("partials/header",{title: "menu", category: menu}); %>
<div class="container">
    <div class="container"><a>path</a></div>
    <div class="row" >
        <% menu.forEach(function(category){ %>
            <h1 class="far"><%= category.category %></h1>
            <% category.menuList.forEach(function(menuList){ %>
                <%- include("partials/menu",{ menu: menuList, category: category.category}) %>
            <% }); %>
        <% }); %>
    </div>
</div>
<%- include("partials/footer", {active: "home"}); %>